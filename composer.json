{
  "name": "reyptr27/laravel-inertia-table",
  "description": "Inertia.js Front-end Components for Spatie's Laravel Query Builder",
  "homepage": "https://github.com/reyptr27/laravel-inertia-table",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Reyptr27",
      "email": "reynaldo.ptr27@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/support": "^10.0|^11.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.8",
    "inertiajs/inertia-laravel": "^1.0",
    "orchestra/testbench": "^9.0",
    "phpunit/phpunit": "^11.0",
    "spatie/laravel-query-builder": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "ReyPtr27\\LaravelQueryBuilderInertiaJs\\": "php"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ReyPtr27\\LaravelQueryBuilderInertiaJs\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
    "php-cs-fixer": "vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.php --verbose"
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "ReyPtr27\\LaravelQueryBuilderInertiaJs\\InertiaTableServiceProvider"
      ]
    }
  }
}
